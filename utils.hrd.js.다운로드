function getCookie(cookieName)
{
	var cookieValue=null;
	if(document.cookie){
		var array=document.cookie.split((escape(cookieName)+'='));
		if(array.length >= 2){
			var arraySub=array[1].split(';');
			cookieValue=unescape(arraySub[0]);
		}
	}
	return cookieValue;
}

function setMemCookie(name,value,domain,expiredays){utils.cookie.set(name, value);}
function setCookie(name,value,domain,expiredays)
{
	var todayDate = new Date();
	todayDate.setDate(todayDate.getDate() + expiredays);
	document.cookie = name + "=" + escape(value) + "; path=/; Domain=" + domain + "; expires=" + todayDate.toGMTString() + ";"
}
function delCookie(cookieName){
	document.cookie = cookieName + "=; expires=Thu, 01 Jan 1970 00:00:00 UTC; path=/;";
}



// 임시로 member 사용
$(document).ready(function(){
	if(getCookie('MemInfo')){
		var M = getCookie('MemInfo'); M = decodeURIComponent(M);var objson = JSON.parse(M);
		if(objson.id){
			if(!getCookie('MSPMemInfo')){
				var _data_ = {"memid":objson.id};var data = HrdMemajax('/my.members.cookie',_data_);
				delCookie('MemInfo');if(data._DATA.MemInfo){setMemCookie('MemInfo',data._DATA.MemInfo,'.heraldcorp.com',7);}
				delCookie('MSPMemInfo');if(data._DATA.MSPMemInfo){setMemCookie('MSPMemInfo',data._DATA.MSPMemInfo,'.heraldcorp.com',7);}

			}else{

			}
		}
	}else{
		if(getCookie('MSPMemInfo')){ delCookie('MSPMemInfo');}
		// 로그인 공통
	}
});




