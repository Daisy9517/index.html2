/* SNS */
/*
kakao					js_sns_kakaoLink(), js_sns_kakaoLink_URL()
facebook            js_sns_fbs()
x(twiter)				js_tws_click()
linkedin,				jssns_linkedin()
radet					js_sns_reddit()
link copy            js_copylink

*/
function js_sns_meta(){
	var url =  $("#snsurl").data("code"); var title =  $("#snstitle").data("code");
	var img =  $("#snsimgurl").data("code"); var desc =  $("#snsdesc").data("code");
	$('meta[property=og\\:url]').attr('content', url);
	$('meta[property=og\\:title]').attr('content', title);
	if(img){$('meta[name=og\\:image]').attr('content', img);}
	if(desc){	$('meta[name=description]').attr('content', desc);}
	return true;
}
function js_sns_linkedin(){
	if(js_sns_meta()){
		var url =  $("#snsurl").data("code"); var title =  $("#snstitle").data("code");
		var snsUrl = encodeURIComponent(url);var snsTitle = encodeURIComponent(title);
		var href = 'https://www.linkedin.com/shareArticle?mini=true&&url='+snsUrl+'&title=' + snsTitle;
		var a = window.open(href, 'linkedin','');
		if (a) {a.focus();}
		return;
	}
	return;
}
function js_sns_fbs() {
	if(js_sns_meta()){
		var url =  $("#snsurl").data("code");
		var title =  $("#snstitle").data("code");
		var snsUrl = encodeURIComponent(url);
		var snsTitle = encodeURIComponent(title);
		var fbWin = window.open('https://www.facebook.com/sharer.php?u='+snsUrl,'facebooksharer','toolbar=0,status=0,width=626,height=436');
		return;
	}
	return;
}
function js_tws_click() {
	if(js_sns_meta()){
		var url =  $("#snsurl").data("code"); var title =  $("#snstitle").data("code");
		var snsUrl = encodeURIComponent(url);var snsTitle = encodeURIComponent(title);
		var a = window.open('http://twitter.com/share?text=' + snsTitle + '&url='+snsUrl, 'twitter', 'width=466, height=356');
		if (a) {a.focus();}
		return;
	}
	return;
}
function js_sns_reddit(){
	if(js_sns_meta()){
		var url =  $("#snsurl").data("code"); var title =  $("#snstitle").data("code");
		var snsUrl = encodeURIComponent(url);var snsTitle = encodeURIComponent(title);
		var a = window.open("https://reddit.com/submit?url="+snsUrl+"&snsTitle="+ snsTitle, "", "");
		if (a) {a.focus();}
		return;
	}
	return;
}

function js_copylink(url = document.location.href){
	var copy_link = url;
	window.navigator.clipboard.writeText(copy_link);
}


// 사용할 앱의 JavaScript 키를 설정해 주세요.
$(document).ready(function() {
	if (window.Kakao && !Kakao.isInitialized()) {
		Kakao.init('8d3b78518aaa2eab78761b8bd65fec41'); // 헤경 인증키 사용
	}
});

// 카카오링크 버튼을 생성합니다. 처음 한번만 호출하면 됩니다.
function js_sns_kakaoLink() {
	//Kakao.Share.sendDefault({
	Kakao.Link.sendDefault({
		objectType: 'feed',
		content: {
			title: $("#snstitle").data("code"),
			description:$("#snsdesc").data("code"),
			imageUrl:$("#snsimgurl").data("code"),
			link: {
				mobileWebUrl: "https://mbiz.heraldcorp.com/article/"+$("#snsid").data("code"),
				webUrl: "https://biz.heraldcorp.com/article/"+$("#snsid").data("code"),
			}
		}
	});
}

// 카카오링크 SNSURL 기준으로 버튼을 생성합니다. 처음 한번만 호출하면 됩니다.
function js_sns_kakaoLink_URL() {
	//Kakao.Share.sendDefault({
	Kakao.Link.sendDefault({
		objectType: 'feed',
		content: {
			title: $("#snstitle").data("code"),
			description:$("#snsdesc").data("code"),
			imageUrl:$("#snsimgurl").data("code"),
			link: {
				mobileWebUrl: $("#snsurl").data("code"),
				webUrl: $("#snsurl").data("code"),
			}
		}
	});
}


