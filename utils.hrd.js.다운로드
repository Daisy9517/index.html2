function get_meminfo(){
	if(utils.isMem() == true){
		var M = getCookie('MemInfo'); M = decodeURIComponent(M);var objson = JSON.parse(M);
		return objson;
	}else{
		return "";
	}
}
function get_memmrq(){
	if(utils.isMem() == true){
		var M = getCookie('MemInfo'); M = decodeURIComponent(M);var objson = JSON.parse(M);
		var _datax_ = {'memberid':_data_.id}
		var data = HrdMemajax('/my.mbers.mbrsxq',_datax_);
		return data._DATA['0'].mrq;
	}else{
		return "";
	}
}


