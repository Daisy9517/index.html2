$(document).ready(function(){

	if(utils.isMem() == true){

		// /* 시리즈구독 */
		let $series_sub = $('.series_sub');

		// /* 연재구독 */
		let $serial_sub = $('.serial_sub');

		// /* 기사 페이지 연재.시리즈 구독 */
		let $series_info = $('.series_info');

		//console.log("ready - $series_sub :" + $series_sub  + "| $serial_sub : " + $serial_sub + "|$series_info : " +$series_info.length);

		if($serial_sub.length){
			var _Scode_ = $(".serial_sub").data("code");

			//console.log("* serial_sub " +_Scode_);

			if(_Scode_){
				var _data_ = { "PI_CHANNEL_TYPE":"S","PI_CHANNEL_ID" : _Scode_}
				var data = HrdMemajax('/reporter.info',_data_);
				if ( 0 != parseInt(data._DATA[0]['tcnt'],10)){
					$("#subscribeStyz").addClass('on_subscribe');
					$("#subscribeStyz").html('<span>구독중</span>');
				}
			}
		}else if($series_sub.length){
			var _Scode_ = $(".series_sub").data("code");

			//console.log("# series_sub " +_Scode_);

			if(_Scode_){
				var _data_ = { "PI_CHANNEL_TYPE":"S","PI_CHANNEL_ID" : _Scode_}
				var data = HrdMemajax('/reporter.info',_data_);
				if ( 0 != parseInt(data._DATA[0]['tcnt'],10)){
					$("#subscribeStyz").addClass('on_subscribe');
					$("#subscribeStyz").html('<span>구독중</span>');
				}
			}
		}

		if($series_info.length==1){
			var _Scode_ = $(".series_info").data("code");
			//console.log("series_info " +_Scode_);
			if(_Scode_){
				var _data_ = { "PI_CHANNEL_TYPE":"S","PI_CHANNEL_ID" : _Scode_}
				//console.log(_data_);

				var data = HrdMemajax('/reporter.info',_data_);
				//console.log(data);
				if ( 0 != parseInt(data._DATA[0]['tcnt'],10)){
					$("#subscribeStyz").removeClass('no_subscribe');
					$("#subscribeStyz").addClass('subscribe');
					$("#subscribeStyz").html('<span>구독중</span>');
				}
			}
		}

		/*if(utils.isMem() == true){
		// 연재구독
		let $serial_sub = $('.serial_sub');

		if($serial_sub.length){
			var _Scode_ = $(".serial_sub").data("code");
			if(_Scode_){
				var _data_ = { "PI_CHANNEL_TYPE":"S","PI_CHANNEL_ID" : _Scode_}
				var data = HrdMemajax('/reporter.info',_data_);
				if ( 0 != parseInt(data._DATA[0]['tcnt'],10)){
					$("#subscribeStyz").addClass('on_subscribe');
					$("#subscribeStyz").html('<span>구독중</span>');
				}
			}
		}*/
	}

});

/* 기사 페이지에서 연재/시리즈 체크함 */
function jst_series_check(){

	if(utils.isMem() == true){
		let $series_info = $('.series_info');

		if($series_info.length){
			var _Scode_ = $(".series_info").data("code");
			if(_Scode_){
				var _data_ = { "PI_CHANNEL_TYPE":"S","PI_CHANNEL_ID" : _Scode_};if(!_Scode_){ return false;}
				var data = HrdMemajax('/reporter.info',_data_);

				if ( 0 == parseInt(data._DATA[0]['tcnt'],10)){
					var tmp = HrdMemajax('/reporter.insert',_data_);
					$("#subscribeStyz").removeClass('no_subscribe');
					$("#subscribeStyz").addClass('subscribe');
					$("#series_popup_ing").hide();
					$("#series_popup_end").show();
					$('#pop_message').text('구독 신청되었습니다');
					$("#subscribeStyz").html('<span>구독중</span>');
				}else{
					var tmp = HrdMemajax('/reporter.delete',_data_);
					$("#subscribeStyz").removeClass('subscribe');
					$("#series_popup_ing").hide();
					$("#series_popup_end").show();
					$('#pop_message').text('구독 취소되었습니다.');
					$("#subscribeStyz").html('<span>구독 + </span>');
				}
			}
		}
	}else{
		if(confirm("구독을 위해서는 로그인을 하셔야 합니다.\n로그인 하시겠습니까?")){
			self.location.replace(_login_url_);
		}else{
			return false;
		}
	}

}

