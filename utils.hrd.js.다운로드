function jst_id(_id_) { return document.getElementById(_id_) ? document.getElementById(_id_):alert('NOT ID'); }
function jst_id_value(_id_){	return jst_id(_id_).value.replace(/\s/g,'');}
function jst_typeof(_id_){		return document.getElementById(_id_) ? true:false; }
function jst_obj(obj){	var str = ""; for(key in obj) { str += key+"="+obj[key]+"\n";	} return str;}

function jst_alphanum(_id_){ return jst_id_value(_id_).replace(/[^0-9a-zA-Z]/g, ''); }
function jst_numonly(_id_){ return jst_id_value(_id_).replace(/[^0-9]/g, ''); }
function jst_num_only(string){ return string.replace(/[^0-9]/g,''); }
function jst_onfocus(_id_)	{	jst_id(_id_).focus(); return; }

function HrdMemInfo(){
	return (utils.isMem()==true) ? JSON.parse(decodeURIComponent(utils.cookie.get('MemInfo'))):'';
}
function HrdMemInfoId(){
	return (utils.isMem()==true) ? JSON.parse(decodeURIComponent(utils.cookie.get('MemInfo'))).id:'';
}
function Hrdajax(url,data,n){
	var _data_ = utils.req.get({
		url: url,datatype:'json',data:data,async : false,
		success: function (response){return (Array.isArray(response._DATA) && response._DATA[0] !== false) ? true:false;},
		error: function (response) {return false;},
	},true);
	return _data_.responseJSON;
}
function HrdMemajax(url,data){
	var _data_ = utils.req.post({
		url: url,datatype:'json',data:data,async : false,
		success: function (response){return (Array.isArray(response._DATA) && response._DATA[0] !== false) ? true:false;},
		error: function (response) {return false;},
	},true);
	return _data_.responseJSON;
}
function hrdStorageset(k,v){ window.localStorage.setItem(k,v);}
function hrdStorageget(k){	 return (window.localStorage.getItem(k)) ?window.localStorage.getItem(k):'';}
function hrdStoragedel(k){	window.localStorage.removeItem(k);}
function hrdStorageclear(){ window.localStorage.clear()}
function browseruuid() {var nav = window.navigator;var screen = window.screen;var guid = nav.mimeTypes.length;guid += nav.userAgent.replace(/\D+/g, '');guid += nav.plugins.length;guid += nav.language.length;guid += nav.hardwareConcurrency;guid += screen.height || '';guid += screen.width || '';guid += screen.pixelDepth || '';return guid;};
